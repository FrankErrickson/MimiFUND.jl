<?xml version="1.0" encoding="utf-8" ?>
<Simulation>

  <GlobalParameters>
    <!-- Use either "DotNet" or "MersenneTwister" here -->
    <RandomNumberGenerator>MersenneTwister</RandomNumberGenerator>
    <Randomize>false</Randomize>
    <MonteCarloRuns>0</MonteCarloRuns>

    <OutputDisaggregatedData>false</OutputDisaggregatedData>
    <OutputVerbal>true</OutputVerbal>
    <OutputInputParameters>false</OutputInputParameters>

    <YearsToAggregate>1000</YearsToAggregate>
    <WeightingCombination>Standard</WeightingCombination>
  </GlobalParameters>

  <Scenarios>
    <Scenario name="FUND">
      <ExcelFile>Parameter - Base.xlsm</ExcelFile>
    </Scenario>
    <Scenario name="SRES A1b">
      <ExcelFile>Parameter - Base.xlsm</ExcelFile>
      <ExcelFile>Parameter - SRES A1b.xlsm</ExcelFile>
    </Scenario>
    <Scenario name="SRES A2">
      <ExcelFile>Parameter - Base.xlsm</ExcelFile>
      <ExcelFile>Parameter - SRES A2.xlsm</ExcelFile>
    </Scenario>
    <Scenario name="SRES B1">
      <ExcelFile>Parameter - Base.xlsm</ExcelFile>
      <ExcelFile>Parameter - SRES B1.xlsm</ExcelFile>
    </Scenario>
    <Scenario name="SRES B2">
      <ExcelFile>Parameter - Base.xlsm</ExcelFile>
      <ExcelFile>Parameter - SRES B2.xlsm</ExcelFile>
    </Scenario>
  </Scenarios>

  <Runs>
    <Run>
      <Scenario>FUND</Scenario>
      <Mode>Marginal</Mode>
    </Run>
  </Runs>
</Simulation>
