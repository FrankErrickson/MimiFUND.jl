<?xml version="1.0" encoding="utf-8" ?>
<Simulation>

  <GlobalParameters>
    <Randomize>false</Randomize>
    <MonteCarloRuns>0</MonteCarloRuns>

    <OutputDisaggregatedData>false</OutputDisaggregatedData>
    <OutputVerbal>true</OutputVerbal>
    <OutputInputParameters>false</OutputInputParameters>

    <YearsToAggregate>300</YearsToAggregate>
    <WeightingCombination>Standard</WeightingCombination>
  </GlobalParameters>

  <Scenarios>
    <Scenario name="B1">
      <ExcelFile>Parameter - Base.xlsm</ExcelFile>
      <ExcelFile>Parameter - SRES B1.xlsm</ExcelFile>
    </Scenario>
  </Scenarios>

  <Runs>
    <Run>
      <Scenario>B1</Scenario>
      <Mode>Marginal</Mode>
    </Run>
  </Runs>
</Simulation>